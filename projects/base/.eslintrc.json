{
	"root": false,
	"overrides": [
		{
			"files": ["*.ts"],
			"extends": ["./../../.eslintrc.json"],
			"rules": {
				"@angular-eslint/directive-selector": [
					"error",
					{ "type": "attribute", "prefix": "base", "style": "camelCase" }
				],
				"@angular-eslint/component-selector": [
					"error",
					{ "type": "element", "prefix": "base", "style": "kebab-case" }
				]
			}
		}
	]
}
